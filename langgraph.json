{
  "$schema": "https://langgra.ph/schema.json",
  "python_version": "3.13",
  "dependencies": [
    "./demo/langgraph",
    "./crates/langsmith-sandbox-py"
  ],
  "graphs": {
    "agent": {
      "path": "./demo/langgraph/agent.py:graph",
      "description": "minimal graph for dev server startup"
    }
  },
  "http": {
    "app": "./demo/langgraph/sandbox_sessions_app.py:app",
    "enable_custom_route_auth": true,
    "disable_mcp": false,
    "disable_a2a": false
  }
}
