{
  "$schema": "https://langgra.ph/schema.json",
  "python_version": "3.13",
  "dockerfile_lines": [
    "RUN apt-get update && apt-get install -y --no-install-recommends build-essential pkg-config && rm -rf /var/lib/apt/lists/*"
  ],
  "dependencies": [
    "./demo/langgraph",
    "./crates/langsmith-sandbox",
    "./crates/langsmith-sandbox-py"
  ],
  "graphs": {
    "agent": {
      "path": "./demo/langgraph/agent.py:graph",
      "description": "minimal graph for dev server startup"
    }
  },
  "http": {
    "app": "./demo/langgraph/sandbox_sessions_app.py:app",
    "enable_custom_route_auth": true,
    "disable_mcp": false,
    "disable_a2a": false
  }
}
